# Hive连接环境变量示例
# 复制此文件为 .env 并修改为您的实际配置

# Hive服务器配置
HIVE_HOST=localhost
HIVE_PORT=10000
HIVE_USERNAME=default
HIVE_DATABASE=default
HIVE_AUTH=NONE

# S3配置（可选）
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=your_access_key
S3_SECRET_KEY=your_secret_key

# 其他配置
USE_BEELINE=false
CONNECTION_TIMEOUT=30
QUERY_TIMEOUT=300
RETRY_ATTEMPTS=3
